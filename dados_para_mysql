Tabelas criadas no mysql do snep
Usar para criar um script!


CREATE TABLE senha ( id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, senha VARCHAR(21) NOT NULL, usuario VARCHAR(100) NOT NULL, grupo VARCHAR(100) NOT NULL, cadastro VARCHAR(50) NOT NULL, atualizado VARCHAR(50) NOT NULL); 
CREATE TABLE rfid ( id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, rfid VARCHAR(150) NOT NULL, grupo VARCHAR(100) NOT NULL, cadastro VARCHAR(50) NOT NULL, atualizado VARCHAR(50) NOT NULL); 
CREATE TABLE tb_porteiro (id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, ip VARCHAR(31) NOT NULL, porta VARCHAR(7) NOT NULL, transporte VARCHAR(12) NOT NULL, mac VARCHAR(31) NOT NULL, nome VARCHAR(100) NOT NULL, rele1 VARCHAR(20) NOT NULL, rele2 VARCHAR(20) NOT NULL, cadastro VARCHAR(50) NOT NULL, atualizado VARCHAR(50) NOT NULL);
CREATE TABLE tb_grupo ( id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, grupo VARCHAR(100) NOT NULL, cadastro VARCHAR(50) NOT NULL, atualizado VARCHAR(50) NOT NULL);
