Tabelas criadas no mysql do snep


CREATE TABLE senha ( id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, senha VARCHAR(21) NOT NULL, usuario VARCHAR(100) NOT NULL, grupo VARCHAR(100) NOT NULL);
CREATE TABLE porteiros (id INT NOT NULL PRIMARY KEY AUTO_INCREMENT, ip VARCHAR(100) NOT NULL, porta VARCHAR(100) NOT NULL, transporte VARCHAR(100) NOT NULL, mac VARCHAR(100) NOT NULL, nome VARCHAR(100) NOT NULL, rele1 VARCHAR(100) NOT NULL, rele2 VARCHAR(100) NOT NULL);

